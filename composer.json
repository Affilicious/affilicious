{
  "name": "affilicious/affilicious-plugin",
  "license": "GPL-2.0 or later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "affiliate",
    "produkte",
    "product",
    "price comparison"
  ],
  "autoload": {
    "psr-4": {
      "Affilicious\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/affilicious/carbon-fields"
    }
  ],
  "require": {
    "php": ">=5.6",
    "pimple/pimple": "^3.0",
    "htmlburger/carbon-fields": "1.4.1",
    "guzzlehttp/guzzle": "6.0",
    "exeu/apai-io": "~2.0",
    "symfony/dom-crawler": "^3.2",
    "webmozart/assert": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
