{
  "name": "affilicioustheme/affilicious",
  "license": "GPL-2.0 or later",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "affiliate",
    "produkte",
    "product",
    "price comparison"
  ],
  "config": {
    "platform": {
      "php": "5.6"
    }
  },
  "autoload": {
    "psr-4": {
      "Affilicious\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Affilicious\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/affilicious/carbon-fields"
    }
  ],
  "require": {
    "php": ">=5.6",
    "pimple/pimple": "^3.0",
    "htmlburger/carbon-fields": "1.4.2",
    "exeu/apai-io": "~2.1",
    "webmozart/assert": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "paulgibbs/behat-wordpress-extension": "dev-master",
    "behat/mink-goutte-driver": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
